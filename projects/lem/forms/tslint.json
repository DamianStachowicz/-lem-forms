{
    "extends": "../../../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "lem-forms",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "lem-forms",
            "kebab-case"
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ]
    }
}
